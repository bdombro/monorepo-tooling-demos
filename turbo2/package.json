{
  "name": "turbo",
  "private": true,
  "scripts": {
    "bootstrap": "pnpm install",
    "bootstrap:with-up:": "pnpm --filter '@mydomain/app...' install"
  },
  "example-commands": {
    "build": "turbo build",
    "build:scoped": "turbo build --filter='@mydomain/app...'",
    "build:changed": "turbo build --filter='[<since>]'",
    "clean": "turbo clean",
    "exec": "turbo exec pwd",
    "lint": "turbo lint",
    "test": "turbo test",
    "watch": "turbo watch --parallel"
  },
  "devDependencies": {
    "turbo": "latest"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "npm@8.19.2"
}
