{
  "name": "turbo",
  "private": true,
  "scripts": {
    "bootstrap": "pnpm install",
    "bootstrap:with-up:": "pnpm --filter '@app/app...' install"
  },
  "example-commands": {
    "build": "turbo build",
    "build:scoped": "turbo build --filter='@app/app...'",
    "build:changed": "turbo build --filter='[<since>]'",
    "clean": "turbo clean",
    "exec": "pnpm --filter '*' exec pwd",
    "lint": "turbo lint",
    "test": "turbo test",
    "watch": "turbo watch --parallel"
  },
  "devDependencies": {
    "turbo": "1.11.3"
  },
  "packageManager": "pnpm@8.9.0",
  "engines": {
    "node": ">=18"
  }
}
