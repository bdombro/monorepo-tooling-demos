{
  "name": "yarn-workspaces",
  "version": "1.0.0",
  "license": "ISC",
  "packageManager": "yarn@4.0.2",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "bootstrap": "yarn",
    "bootstrap:with-up:": "yarn workspaces focus app",
    "build": "yarn workspaces foreach -Avt run build",
    "build:scoped": "yarn app build",
    "build:scoped-with-upstream": "yarn workspaces foreach -Rt --from app run build",
    "build:changed": "yarn workspaces foreach -Av --since run build",
    "clean": "yarn workspaces foreach -Avp run clean",
    "clear-resolution-cache": "rm -rf ~/.yarn/berry/metadata/npm",
    "exec": "yarn workspaces foreach -At exec pwd",
    "lint": "yarn workspaces foreach -At run lint",
    "test": "yarn workspaces foreach -Atv run test",
    "watch": "yarn workspaces foreach -Av run watch"
  },
  "dependencies": {
    "typescript": "^5.3.3"
  }
}
