{
  "name": "lerna-2x",
  "version": "1.0.0",
  "license": "ISC",
  "scripts": {
    "bootstrap": "lerna bootstrap",
    "bootstrap:with-up": "lerna bootstrap --scope=@app/lib2 --include-filtered-dependencies",
    "bootstrap:with-down": "lerna bootstrap --scope=@app/lib2 --include-filtered-dependents",
    "build": "lerna run build --stream",
    "build:scoped": "lerna run build --scope=app --include-filtered-dependencies --include-filtered-dependents --stream",
    "build:changed": "lerna run build --since --sort",
    "clean": "lerna clean --yes && lerna run clean --parallel",
    "exec": "lerna exec --parallel -- pwd",
    "lint": "lerna run lint --parallel",
    "test": "lerna run test --parallel",
    "watch": "lerna run watch --parallel"
  },
  "devDependencies": {
    "lerna": "2.11.0"
  },
  "packageManager": "yarn@1.22.2"
}
